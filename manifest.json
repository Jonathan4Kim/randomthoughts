{
    "manifest_version": 3,
    "name": "Random Quote New Tab",
    "version": "1.0",
    "description": "Displays a random quote every time you open a new tab.",
    "permissions": ["scripting"],
    "host_permissions": ["https://favqs.com/"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    }
  }
  